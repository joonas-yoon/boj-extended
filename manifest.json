{
  "name": "BOJ Extended",
  "description": "백준 온라인 저지(BOJ)를 확장된 기능과 함께 사용해보세요.",
  "version": "1.2.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.acmicpc.net/?", "https://www.acmicpc.net/*"],
      "all_frames": true,
      "css": ["css/theme-dark.css"],
      "js": [
        "js/db.js",
        "js/config.js",
        "js/utils.js",
        "js/bg-theme.js",
        "js/bg-user.js",
        "js/bg-status.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": ["css/common.css", "css/user.css"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"]
}
